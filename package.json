{
  "name": "verktyg",
  "version": "1.0.0",
  "description": "Useful verktyg for building websites",
  "author": "Robin Isaksson <robin.isaksson@gmail.com> (http://robinisaksson.com)",
  "license": "MIT",
  "main": "dist/verktyget.js",
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "core-js": "^2.5.1",
    "cpx": "^1.5.0",
    "eslint": "^3.10.2",
    "uglify-js": "^3.0.23",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run watch:js",
    "build": "npm run build:js",
    "watch:js": "watchify src/index.js --debug --verbose --outfile dist/verktyget.js --transform [ babelify --only src --presets env ]",
    "build:js": "browserify src/index.js --transform [ babelify --only src --presets env ] | uglifyjs -c > dist/verktyget.js",
    "eslint": "eslint src/js/**; exit 0"
  },
  "keywords": [
    "scroll-animation",
    "ajax-loading",
    "json-loading",
    "image-loading",
    "video-loading",
    "video-player",
    "video-player",
    "dom tools"
  ]
}
